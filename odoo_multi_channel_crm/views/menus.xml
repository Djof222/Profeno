<odoo>
	<data>
		<record id="action_dashboard" model="ir.actions.client">
			<field name="name">Multi-Channel CRM Dashboard</field>
			<field name="tag">tag_dashboard</field>
		</record>

		<menuitem
			id="root_menu_odoo_multi_channel_crm"
			web_icon="odoo_multi_channel_crm,static/description/icon.png"
			name="Multi-CRM"
			groups="group_user"
		/>
			<menuitem
				id="menu_dashboard"
				parent="root_menu_odoo_multi_channel_crm"
				name="Dashboard"
				action="action_dashboard"
				sequence="10"
			/>
			<menuitem
				id="menu_mapping"
				name="Mapping"
				parent="root_menu_odoo_multi_channel_crm"
				sequence="30"
			/>
			<menuitem
				id="menu_channel_feed"
				name="Feed"
				parent="root_menu_odoo_multi_channel_crm"
				sequence="40"
				groups="base.group_no_one"
			/>
	</data>
</odoo>
