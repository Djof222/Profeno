<odoo>
	<data>
		<record id="tree_lead_mapping" model="ir.ui.view">
			<field name="name">crm.lead.mapping.tree</field>
			<field name="model">crm.lead.mapping</field>
			<field name="arch" type="xml">
				<tree decoration-danger="is_modified">
					<field name="is_modified" column_invisible="True"/>
					<field name="connection_id"/>
					<field name="channel"/>
					<field name="remote_id"/>
					<field name="local_id"/>
					<field name="imported"/>
				</tree>
			</field>
		</record>

		<record id="form_lead_mapping" model="ir.ui.view">
			<field name="name">crm.lead.mapping.form</field>
			<field name="model">crm.lead.mapping</field>
			<field name="arch" type="xml">
				<form archive="false">
					<sheet>
						<group>
							<group>
								<field name="is_modified" invisible="True"/>
								<field name="connection_id"/>
								<field name="channel"/>
								<field name="imported"/>
								<field name="remote_id"/>
								<field name="local_id"/>
							</group>
						</group>
						<separator string="Logs Store Ids" groups="base.group_no_one"/>
						<field name="log_ids" widget="many2many_tags" readonly="True" groups="base.group_no_one"/>
					</sheet>
				</form>
			</field>
		</record>

		<record id="action_lead_mapping" model="ir.actions.act_window">
			<field name="name">Lead Mappings</field>
			<field name="res_model">crm.lead.mapping</field>
			<field name="view_mode">tree,form</field>
		</record>

		<menuitem
			id="menu_lead_mapping"
			name="Lead"
			action="action_lead_mapping"
			parent="menu_mapping"
			sequence="30"
		/>
	</data>
</odoo>
