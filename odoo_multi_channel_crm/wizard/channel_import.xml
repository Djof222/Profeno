<odoo>
	<data>
		<record id="form_channel_import_wizard" model="ir.ui.view">
			<field name="name">channel.import.form</field>
			<field name="model">channel.import</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<group>
								<field name="connection_id"/>
								<field name="channel" invisible="True"/>
							</group>
							<group>
								<field name="object_type"/>
							</group>
							<group name="filter_type"/>
							<group name="filter"/>
						</group>
					</sheet>
					<footer>
						<button
							string="Import"
							type="object"
							name="import_button"
							class="oe_highlight btn btn-success"
						/>
						<button
							string="Cancel"
							class="btn btn-danger"
							name="cancel"
							special="cancel"
						/>
					</footer>
				</form>
			</field>
		</record>

		<record id="action_channel_import_wizard" model="ir.actions.act_window">
			<field name="name">Import from CRM</field>
			<field name="res_model">channel.import</field>
			<field name="view_id" ref="form_channel_import_wizard"/>
			<field name="target">new</field>
		</record>
	</data>
</odoo>
