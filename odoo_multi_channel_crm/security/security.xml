<odoo>
	<data noupdate="0">
		<record id="category_groups_multi_crm" model="ir.module.category">
			<field name="name">Multi-CRM</field>
			<field name="description">Helps you manage your crm connections.</field>
		</record>

		<record id="group_user" model="res.groups">
			<field name="name">User</field>
			<field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
			<field name="category_id" ref="category_groups_multi_crm"/>
		</record>

		<record id="group_manager" model="res.groups">
			<field name="name">Administrator</field>
			<field name="category_id" ref="category_groups_multi_crm"/>
			<field name="implied_ids" eval="[(4, ref('group_user'))]"/>
			<field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
		</record>
	</data>
</odoo>
